<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mypokerassistant">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HeadsUpPokerStats"
        tools:targetApi="31">
        <activity
            android:name="com.example.mypokerassistant.MyPokerAssistantActivities.HandGuideActivity"
            android:parentActivityName="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity" />
        </activity>
        <activity
            android:name="com.example.mypokerassistant.MyPokerAssistantActivities.WinningOddsActivity"
            android:parentActivityName="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity" />
        </activity>
        <activity
            android:name="com.example.mypokerassistant.MyPokerAssistantActivities.HandRankingActivity"
            android:parentActivityName="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity" />
        </activity>
        <activity
            android:name="com.example.mypokerassistant.MyPokerAssistantActivities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>